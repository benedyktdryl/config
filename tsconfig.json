{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "declaration": true,
    "lib": [
      "es2016",
      "es2017.object"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": true,
    "target": "es5",
    "preserveConstEnums": true,
    "typeRoots": [
      "./node_modules/@types"
    ]
  }
}